{
  "routes": [
    {
      "src": "/tasks/task-edit/(.*)",
      "dest": "/api/prerender.js?taskId=$1"
    }
  ],
  "rewrites": [
    { "source": "/((?!tasks/task-edit/).*)", "destination": "/index.html" }
  ],
  "functions": {
    "api/prerender.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
